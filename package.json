{
  "name": "delta-launcher",
  "version": "1.3.0",
  "description": "aaaaaaa",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=assets/icons/mac/icon.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . --help",
    "package-linux": "electron-packager . delta-launcher --overwrite --asar=true --platform=linux --arch=x64 --icon=buildResources/icon.ico --prune=true --out=release-builds",
    "pack": "electron-builder --dir",
    "dist": "electron-builder"
  },
  "author": "Albee",
  "license": "MIT",
  "dependencies": {
    "adm-zip": "^0.4.16",
